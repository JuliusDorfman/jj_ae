    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="assets/css/style.css">
    <script src="javascript/clientSideJS.js"></script>
</head>

<body>
    <!-- Navbar -->
    <div id="myNavbar" class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="/moodmusic" class="navbar-brand" id="headerLogo">moodmusic</a>
            </div>
            <ul class="nav navbar-nav navbar-right navbar-collapse collapse">
                <li data-toggle="modal" id="navBar"><a id="navBar" href="/moodmusic" onclick="deleteAllCookies()">Log Out</a></li>
                <li data-toggle="modal" id="navBar"><a id="navBar" href="/login">Log In</a></li>
                <li class data-toggle="modal" data-target="#aboutModal" id="modalButtons">About</li>
                <li class data-toggle="modal" data-target="#statsModal" id="modalButtons">Stats</li>
            </ul>
        </div>
    </div>
    <!--    <div class="modal fade" id="logInModal" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">About moodmusic</h4>
                </div>
                <div class="modal-body">
                    <img src="/assets/images/About.png">
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div> -->
    <!-- About Modal -->
    <div class="modal fade" id="aboutModal" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">About moodmusic</h4>
                </div>
                <div class="modal-body">
                    <img src="/assets/images/About.png">
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <!-- Stats Modal -->
    <div class="modal fade" id="statsModal" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Stats</h4>
                </div>
                <div class="modal-body">
                    <div class="container-fluid">
                        <div class="row">
                            <li id="averageSongValence">Average Song Valence: </li>{{song.valence}}
                            <br>
                            <li id="someAverageStat">Other Stat: </li>{{song.song_name}}
                            <br>
                            <li id="someAverageStat2">Other Stat: </li>{{song.current_user}}
                            <br>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{{body}}}
</body>

</html>